# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Upload Configuration
# The app supports up to 200MB file uploads for images and content
# This is configured in supabase/config.toml and throughout the application

# Google OAuth Configuration (for Google Site Kit)
VITE_GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id.apps.googleusercontent.com
VITE_GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret
VITE_GOOGLE_OAUTH_REDIRECT_URI=https://wecandotoo.com/admin

# Google Services Configuration (Optional - for static tracking)
# Your actual AdSense Publisher ID and Customer ID
VITE_GOOGLE_ADSENSE_CLIENT_ID=ca-pub-2959602333047653
VITE_GOOGLE_ADSENSE_CUSTOMER_ID=9592425312
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_GOOGLE_SITE_VERIFICATION=your_verification_code

# Feature Flags - Set to 'true' to enable Google services
VITE_ENABLE_GOOGLE_ANALYTICS=false
VITE_ENABLE_GOOGLE_ADSENSE=false
VITE_ENABLE_GOOGLE_SITE_VERIFICATION=false
